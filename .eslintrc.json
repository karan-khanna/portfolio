{
  "settings": {
    "import/resolver": {
      "alias": []
    }
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["plugin:import/react", "airbnb"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "parser": "babel-eslint",
  "plugins": ["react", "react-hooks"]
}
